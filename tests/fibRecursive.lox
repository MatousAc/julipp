fun fib(n) {
  if (n <= 1) {
    print "in :  " + n;
    return n;
  }
  print "out: " + n;
  res = fib(n - 2) + fib(n - 1);
  print "nis: " + n;
  print "ret: " + res;
  return res;
}

for (var i = 0; i < 10; i = i + 1) {
  print "i:   " + i;
  print fib(i);
}


