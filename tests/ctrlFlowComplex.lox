// something more complex
var i = 10;
for (var j = 0; j < 4; j = j + 1) {
	print "j is " + j;
	if (j == 0) continue;
	while (i != 6) {
		print "i is " + i;
		if (i == 9) {
			i = i - 1;
			continue;
		}
		if (i == 8 and j > 1) break;
		i = i - 1;
		print "hi";
	}
	i = 10;
	if (j == 3) break;
	print "end";
}
